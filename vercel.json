{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "regions": ["cdg1"]  // Spécifiez ici la ou les régions souhaitées
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.py",
      "regions": ["cdg1"]  // Spécifiez ici la ou les régions souhaitées
    }
  ]
}
